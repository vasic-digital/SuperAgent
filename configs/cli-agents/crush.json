{
  "$schema": "https://charm.land/crush.json",
  "providers": {
    "helixagent": {
      "name": "HelixAgent AI Debate Ensemble",
      "type": "openai",
      "base_url": "http://localhost:7061/v1",
      "api_key": "sk-bd15ed2af3d6cd8c0bdf57e221bbf7771fa06bda93cc8866807cc85211f58d1a",
      "models": [
        {
          "id": "helixagent-debate",
          "name": "HelixAgent Debate Ensemble",
          "cost_per_1m_in": 0,
          "cost_per_1m_out": 0,
          "context_window": 128000,
          "default_max_tokens": 8192,
          "can_reason": true,
          "supports_attachments": true,
          "streaming": true,
          "supports_brotli": true,
          "options": {
            "embeddings": true,
            "function_calls": true,
            "image_input": true,
            "image_output": true,
            "ocr": true,
            "pdf": true,
            "tool_use": true,
            "vision": true
          }
        }
      ]
    }
  },
  "lsp": {
    "helixagent-lsp": {
      "command": "curl -X POST http://localhost:7061/v1/lsp",
      "args": [
        "-H",
        "Authorization: Bearer sk-bd15ed2af3d6cd8c0bdf57e221bbf7771fa06bda93cc8866807cc85211f58d1a"
      ],
      "enabled": true
    }
  },
  "mcp": {
    "everything": {
      "type": "local",
      "command": [
        "npx",
        "-y",
        "@modelcontextprotocol/server-everything"
      ],
      "enabled": true
    },
    "fetch": {
      "type": "local",
      "command": [
        "uvx",
        "mcp-server-fetch"
      ],
      "enabled": true
    },
    "filesystem": {
      "type": "local",
      "command": [
        "npx",
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "/home/milosvasic"
      ],
      "enabled": true
    },
    "git": {
      "type": "local",
      "command": [
        "uvx",
        "mcp-server-git"
      ],
      "enabled": true
    },
    "github": {
      "type": "local",
      "command": [
        "npx",
        "-y",
        "@modelcontextprotocol/server-github"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "{env:GITHUB_TOKEN}"
      },
      "enabled": true
    },
    "helixagent": {
      "type": "local",
      "command": [
        "node",
        "/home/milosvasic/.helixagent/plugins/mcp-server/dist/index.js",
        "--endpoint",
        "http://localhost:7061"
      ],
      "enabled": true
    },
    "helixagent-acp": {
      "type": "remote",
      "url": "http://localhost:7061/v1/acp",
      "enabled": true
    },
    "helixagent-cognee": {
      "type": "remote",
      "url": "http://localhost:7061/v1/cognee",
      "enabled": true
    },
    "helixagent-embeddings": {
      "type": "remote",
      "url": "http://localhost:7061/v1/embeddings",
      "enabled": true
    },
    "helixagent-lsp": {
      "type": "remote",
      "url": "http://localhost:7061/v1/lsp",
      "enabled": true
    },
    "helixagent-mcp": {
      "type": "remote",
      "url": "http://localhost:7061/v1/mcp",
      "enabled": true
    },
    "helixagent-vision": {
      "type": "remote",
      "url": "http://localhost:7061/v1/vision",
      "enabled": true
    },
    "memory": {
      "type": "local",
      "command": [
        "npx",
        "-y",
        "@modelcontextprotocol/server-memory"
      ],
      "enabled": true
    },
    "puppeteer": {
      "type": "local",
      "command": [
        "npx",
        "-y",
        "@modelcontextprotocol/server-puppeteer"
      ],
      "enabled": true
    },
    "sequential-thinking": {
      "type": "local",
      "command": [
        "npx",
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "enabled": true
    },
    "sqlite": {
      "type": "local",
      "command": [
        "npx",
        "-y",
        "mcp-server-sqlite-npx",
        "/tmp/helixagent.db"
      ],
      "enabled": true
    },
    "time": {
      "type": "local",
      "command": [
        "uvx",
        "mcp-server-time"
      ],
      "enabled": true
    }
  },
  "options": {}
}