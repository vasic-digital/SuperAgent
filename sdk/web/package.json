{
  "name": "helixagent-sdk",
  "version": "1.0.0",
  "description": "HelixAgent JavaScript/TypeScript SDK for AI orchestration platform",
  "main": "dist/helixagent.js",
  "module": "dist/helixagent.esm.js",
  "types": "dist/helixagent.d.ts",
  "browser": "dist/helixagent.browser.js",
  "exports": {
    ".": {
      "types": "./dist/helixagent.d.ts",
      "import": "./dist/helixagent.esm.js",
      "require": "./dist/helixagent.js",
      "browser": "./dist/helixagent.browser.js"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm && npm run build:browser && npm run build:types",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:browser": "esbuild src/index.ts --bundle --minify --outfile=dist/helixagent.browser.js --platform=browser --target=es2020",
    "build:types": "tsc -p tsconfig.types.json",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts,.js",
    "format": "prettier --write 'src/**/*.ts'",
    "prepublishOnly": "npm run build && npm test"
  },
  "keywords": [
    "helixagent",
    "ai",
    "llm",
    "openai",
    "api",
    "sdk",
    "ensemble",
    "multi-model",
    "typescript"
  ],
  "author": "HelixAgent",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://dev.helix.agent.git"
  },
  "homepage": "https://dev.helix.agent#readme",
  "bugs": {
    "url": "https://dev.helix.agent/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "esbuild": "^0.19.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "node-fetch": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "node-fetch": {
      "optional": true
    }
  }
}
